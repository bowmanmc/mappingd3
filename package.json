{
  "name": "mappingd3",
  "version": "1.0.0",
  "description": "",
  "author": "bowmanmc",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "clean": "rimraf \"build/*\"",
    "deploy": "gh-pages-deploy",
    "dist": "npm run clean && npm run js && npm run sass && npm run jekyll",
    "jekyll": "jekyll build --source src --destination build",
    "js": "webpack -d --progress --colors",
    "presass": "sass-lint -c .sass-lint.yml -v",
    "sass": "node-sass --source-map true --include-path src/sass --include-path node_modules src/sass/main.scss src/styles/bundle.css",
    "postsass": "postcss -c postcss.config.json",
    "server:dev": "babel-node tools/srcServer.js",
    "prestart": "npm-run-all --parallel clean start-message",
    "start": "npm-run-all --parallel open:src sass:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "parallelshell \"npm run watch:jekyll\" \"npm run watch:js\" \"npm run watch:sass\"",
    "watch:jekyll": "nodemon -w src -e html,md,png,jpg,gif,svg,yml,xml,js,css --ignore src/javascript --delay 1 -x \"npm run jekyll\"",
    "watch:js": "webpack -d --progress --colors --watch",
    "watch:sass": "nodemon -e scss -w src/sass -x \"npm run sass\""
  },
  "gh-pages-deploy": {
    "staticpath": "build",
    "prep": [
      "build"
    ],
    "noprompt": true
  },
  "dependencies": {
    "babel-preset-es2015": "^6.18.0",
    "bourbon": "^4.2.7",
    "bourbon-neat": "^1.8.0",
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^5.8.38",
    "babel-loader": "^5.4.2",
    "browser-sync": "^2.18.2",
    "chalk": "^1.1.3",
    "cssnano": "^3.8.0",
    "del": "^2.2.2",
    "gh-pages-deploy": "^0.4.2",
    "gulp": "^3.9.1",
    "http-proxy-middleware": "^0.9.1",
    "moment": "^2.16.0",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.0.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.6.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "script-loader": "^0.6.1",
    "sync-exec": "^0.6.2",
    "webpack": "^1.13.3"
  }
}
